{
  "name": "schema-gen",
  "version": "1.0.0",
  "description": "Generate cesium-native C++ classes from a JSON schema",
  "main": "index.js",
  "scripts": {
    "generate-3d-tiles": "node index.js --schema https://raw.githubusercontent.com/CesiumGS/3d-tiles/3d-tiles-next/specification/schema/tileset.schema.json --output ../../Cesium3DTiles --extensions https://raw.githubusercontent.com/CesiumGS/3d-tiles/3d-tiles-next/extensions/ --namespace Cesium3DTiles --config 3dTiles.json",
    "generate-gltf": "node index.js --schema https://raw.githubusercontent.com/CesiumGS/glTF/3d-tiles-next/specification/2.0/schema/glTF.schema.json --output ../../CesiumGltf --extensions https://raw.githubusercontent.com/CesiumGS/glTF/3d-tiles-next/extensions/2.0/ --namespace CesiumGltf --config glTF.json",
    "test-3d-tiles": "node index.js --schema https://raw.githubusercontent.com/CesiumGS/3d-tiles/3d-tiles-next/specification/schema/tileset.schema.json --output test_output --extensions https://raw.githubusercontent.com/CesiumGS/3d-tiles/3d-tiles-next/extensions/ --namespace Cesium3DTiles --config 3dTiles.json",
    "test-gltf": "node index.js --schema https://raw.githubusercontent.com/CesiumGS/glTF/3d-tiles-next/specification/2.0/schema/glTF.schema.json --output test_output --extensions https://raw.githubusercontent.com/CesiumGS/glTF/3d-tiles-next/extensions/2.0/ --namespace CesiumGltf --config glTF.json"
  },
  "author": "CesiumGS, Inc.",
  "license": "UNLICENSED",
  "dependencies": {
    "lodash": "^4.17.20",
    "handlebars": "^4.7.7",
    "sync-request": "^6.1.0",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "prettier": "^2.2.1"
  }
}
